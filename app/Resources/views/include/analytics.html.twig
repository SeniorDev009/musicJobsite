<script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-35668621-1', 'vocalizr.com');
    ga('send', 'pageview');
    ga('require', 'GTM-N7W3CW2');

    {% if app.user %}
        ga('set', 'dimension1', 'Member' );
        {% if app.user.isSubscribed %}
            ga('set', 'dimension2', 'PRO' );
        {% else %}
            ga('set', 'dimension2', 'FREE' );
        {% endif %}

        {% if app.user.isProducer and app.user.isVocalist %}
            ga('set', 'dimension3', 'Vocalist & Producer' );
        {% elseif app.user.isProducer %}
            ga('set', 'dimension3', 'Producer' );
        {% elseif app.user.isProducer %}
            ga('set', 'dimension3', 'Vocalist' );
        {% endif %}
    {% else %}
        ga('set', 'dimension1', 'Visitor' );
    {% endif %}
</script>