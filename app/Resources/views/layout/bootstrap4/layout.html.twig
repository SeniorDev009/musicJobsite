<!doctype html>
<html lang="en">
<head>
    {% include 'include/analytics.html.twig' %}

    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link rel="icon" type="image/png" href="vfavicon.png">
    <meta property="og:image" content="http://{{ domain_base }}/images/logo_share.png"/>
    <meta property="og:image:secure_url" content="https://{{ domain_base }}/images/logo_share.png" />

    {% stylesheets
        '@VocalizrAppBundle/Resources/public/css/loading-gradient.css'
        '@VocalizrAppBundle/Resources/public/bootstrap4/css/style.css'
        '@VocalizrAppBundle/Resources/public/bootstrap4/css/custom.css'
        '@VocalizrAppBundle/Resources/public/css/waveforms.css'
        '@VocalizrAppBundle/Resources/public/bootstrap4/css/volume-slider.css'
        '@VocalizrAppBundle/Resources/public/bootstrap4/css/toastify.css'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css"/>

    {% block stylesheet %}{% endblock %}

    <title>
        {% set _titleBlock = block('title') %}
        {% if not _titleBlock is empty %}
            {{ _titleBlock }} | Vocalizr
        {% else %}
            Vocalizr
        {% endif %}
    </title>

    {% set _descBlock = block('description') %}
    {% if not _descBlock is empty %}
        <meta name="description" content="{{ _descBlock }}">
    {% else %}
        <meta name="description" content="{{ _titleBlock }}. Connect with vocalists and producers from around the world.">
    {% endif %}

    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.11.2/css/all.css" integrity="sha384-zrnmn8R8KkWl12rAZFt4yKjxplaDaT7/EUkKm7AovijfrQItFWR7O/JJn4DAa/gx" crossorigin="anonymous">
</head>
<body>
{% include 'layout/bootstrap4/include/navigation.html.twig' %}
{% block body %}
{% endblock %}

{% block footer %}
    <!-- Footer -->
    <footer class="footer">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="footer__wrp">
                        <div class="footer__police">
                            <p class="footer__police-text">Â© Copyright Vocalizr {{ 'now'|date('Y') }}</p>
                        </div>
                        <div class="footer__links">
                            <ul class="footer__links-ul">
                                <li class="footer__links-li">
                                    <a href="https://help.vocalizr.com/" class="footer__links-a">Help</a>
                                </li>
                                <li class="footer__links-li">
                                    <a href="https://blog.vocalizr.com/" class="footer__links-a">Blog</a>
                                </li>
                                <li class="footer__links-li">
                                    <a href="{{ path('membership') }}" class="footer__links-a">Membership</a>
                                </li>
                                <li class="footer__links-li">
                                    <a href="{{ path('website_fees') }}" class="footer__links-a">Fees & Charges</a>
                                </li>
                                <li class="footer__links-li">
                                    <a href="{{ path('website_terms') }}" class="footer__links-a">Terms</a>
                                </li>
                                <li class="footer__links-li">
                                    <a href="{{ path('website_privacy') }}" class="footer__links-a">Privacy</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- END Footer -->
{% endblock %}

<!-- Optional JavaScript -->
<!-- jQuery first, then Popper.js, then Bootstrap JS -->
<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>

<!-- Js Slick Slider -->
<script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
<script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>

{% javascripts
    '@VocalizrAppBundle/Resources/public/bootstrap4/js/main.js'
    '@VocalizrAppBundle/Resources/public/js/waveforms.js'
    '@VocalizrAppBundle/Resources/public/js/wavesurfer.min.js'
    '@VocalizrAppBundle/Resources/public/bootstrap4/js/toastify.min.js'
%}
    <script src="{{ asset_url }}"></script>
{% endjavascripts %}

{% block javascript %}{% endblock %}

<div class="modal fade" id="vocalizrModal" tabindex="-1" role="dialog" aria-labelledby="vocalizr-modal" aria-hidden="true"></div>

</body>
</html>